<%= erb :sidebar, layout=>false %>
  <div>
    <ul class="breadcrumb">
      <li>Durations</li>
    </ul>
  </div>
  
  <div class="row-fluid">
    <div class="span12 column" id="col1">
	  <div class="box" id="box1">
	    <h4 class="box-header round-top">期間</h4>
	    <div class="box-container-toggle">
	      <div class="box-content">
	      　　<table class="table table-striped table-condensed" id="races">
	      　　  <tr><th>No.</th><th>type</th><th>name</th><th>Start Date</th><th>End Date</th><th></th></tr>
	      　　  <% @i = 1 %>
	      　　  <% for d in @dur %>
	      　　  <tr>
	      　　    <td><%= @i %></td>
	      　　    <td><%=h d.dtype %></td>
	      　　    <td><%=h d.name %></td>
	      　　    <td><%=h d.start %></td>
	      　　    <td><%=h d.end %></td>
	      　　    <td><i class="icon-pencil"></i>&nbsp;<i class="icon-trash"></i></td>
	      　　  </tr>
	      　　  <% @i += 1 %>
	      　　  <% end %>
	      　　</table>
	      </div><!-- /box-content -->
	    </div><!-- /box-container-toggle -->
      </div><!-- /box -->
    </div><!-- /span12 column -->
  </div><!-- row-fluid -->
  <%= erb :sidebar_close, layout=>false %>
