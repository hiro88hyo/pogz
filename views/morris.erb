    <script>
$(function () {
  $.getJSON(
    "/ranking.json",
    null,
    function(rank_data, status){
      new Morris.Line({
        element: 'hero-graph',
        data: rank_data,
        xkey: 'period',
        smooth: false,
        hideHover: true,
        ykeys: ['だっち', 'ぽっくん', 'トモロ', 'コージ', 'エトゥ', 'しげZ'],
        labels: ['だっち', 'ぽっくん', 'トモロ', 'コージ', 'エトゥ', 'しげZ'],
        lineColors: ['#6666FF','#66CCFF', '#663399', '#33CC33', '#CC3366', '#CCCC33']
      });
    }
  );
  $.ajax({
  	url: "/ranking",
  	cache: false,
  	success: function(html){
  	  $("#ranking").html(html);
  	}
  });
  $("#select01").change(function(){
  	$.ajax({
  	  url: "/ranking?year="+$("#select01").val(),
  	  cache: false,
  	  success: function(html){
  	  	$("#ranking").html(html);
  	  }
  	});
  });
      });
    </script>
